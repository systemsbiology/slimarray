<h2>Enter Hybridizations</h2>

<% if(@available_samples.size > 0) %>
<% form_tag( { :action => 'add' }, { :name => 'add_samples' } ) do -%>
    <%= render :partial => 'form' %>    
  <%= submit_tag 'Add Samples to Hybridization Table' %>
<% end -%>
<% else %>
    <p>There are no submitted samples available for hybridization.</p>
<% end %>

<%= link_to 'Back to List', :action => 'list' %>
